        "debug": "^4.3.4",
        "express": "^4.18.2",
        "express-handlebars": "^7.1.2",
        "hbs": "^4.2.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.8.0",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.28.0"
    },
    "devDependencies": {
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3"
    }
